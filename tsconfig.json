{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",                           /* Specify JSX code generation: 'preserve', 'react-native', or 'react'. */
    "allowJs": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true  ,
    "downlevelIteration": true, 
    "skipLibCheck": true
  },
  
}